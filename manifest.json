{
  "manifest_version": 2,
  "name": "Scheduloader",
  "description": "This extension lets you schedule downloads in the future by right-clicking on them",
  "version": "0.0.1",
  "options_page": "options.html",
  "browser_action": {
    /*"default_icon": "img/icon.png",*/
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "downloads"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  }
}
